{
  "expo": {
    "name": "ELO",
    "slug": "stv-mobile",
    "version": "1.0.6",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "scheme": "ELO",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.stv.mobile.elo",
      "infoPlist": {
        "NSCameraUsageDescription": "Precisamos de acesso à sua câmera para que você possa tirar fotos e enviá-las como anexo nos chamados.",
        "NSPhotoLibraryUsageDescription": "Precisamos de acesso às suas fotos para que você possa selecionar imagens e enviá-las como anexo nos chamados."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0C6F48"
      },
      "package": "com.stv.mobile.elo",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0C6F48"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "99d7cde5-98ed-41af-a83c-a683669f5c3d"
      }
    }
  }
}